<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="viewCompte" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.compte.CompteViewController">
   <top>
      <AnchorPane prefHeight="82.0" prefWidth="600.0" style="-fx-background-color: #968A87;" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="menu" layoutX="34.0" layoutY="33.0" onMouseClicked="#openMenu" text="Menu" />
            <Label layoutX="244.0" layoutY="28.0" text="Gestion Client">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </top>
   <left>
      <AnchorPane fx:id="sliderMenu" prefHeight="316.0" prefWidth="107.0" style="-fx-background-color: #968A87;" BorderPane.alignment="CENTER" />
   </left>
   <center>
      <TabPane fx:id="tablePromo" prefHeight="316.0" prefWidth="430.0" tabClosingPolicy="UNAVAILABLE" />
   </center>
   <right>
      <VBox prefHeight="316.0" prefWidth="34.0">
         <children>
            <ImageView fx:id="ajouterArgentImageView" fitHeight="33.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets bottom="25.0" top="50.0" />
               </VBox.margin>
            </ImageView>
            <ImageView fx:id="ajouterClientImageView" fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets bottom="8.0" />
               </VBox.margin>
            </ImageView>
            <ImageView fx:id="modificationClientImageView" fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets bottom="8.0" />
               </VBox.margin>
            </ImageView>
            <ImageView fx:id="suppressionClientImageView" fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets bottom="25.0" />
               </VBox.margin>
            </ImageView>
            <ImageView fx:id="gestionGroupeImageView" fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true" />
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </right>
</BorderPane>
