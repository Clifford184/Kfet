<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="viewCompte" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.compte.CompteViewController">
   <top>
      <AnchorPane prefHeight="82.0" prefWidth="600.0" style="-fx-background-color: #968A87;" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="menu" layoutX="34.0" layoutY="33.0" onMouseClicked="#openMenu" text="Menu" />
            <Label layoutX="244.0" layoutY="28.0" text="Gestion Client">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </top>
   <left>
      <AnchorPane fx:id="sliderMenu" prefHeight="316.0" prefWidth="107.0" style="-fx-background-color: #968A87;" BorderPane.alignment="CENTER" />
   </left>
   <center>
      <TabPane fx:id="tablePromo" prefHeight="316.0" prefWidth="430.0" tabClosingPolicy="UNAVAILABLE" />
   </center>
   <right>
      <AnchorPane prefHeight="316.0" prefWidth="51.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="ajouterClientImageView" fitHeight="49.0" fitWidth="25.0" layoutX="13.0" layoutY="106.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="ajouterArgentImageView" fitHeight="33.0" fitWidth="29.0" layoutX="11.0" layoutY="144.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </AnchorPane>
   </right>
</BorderPane>
